<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
	<bean id="textEditor" class="com.metacube.training.TextEditor"
		autowire="byName" />
	<bean id="spellChecker1" class="com.metacube.training.SpellChecker">
		<property name="word" value="SpellChecker1" />
	</bean>
	<bean id="spellChecker2" class="com.metacube.training.SpellChecker">
		<property name="word" value="SpellChecker2" />
	</bean>
	<bean id="spellChecker3" class="com.metacube.training.SpellChecker">
		<property name="word" value="SpellChecker3" />
	</bean>
</beans>